        android:supportsRtl="true"
        android:theme="@style/Theme.PruebaPractica2">
        <activity
            android:name=".ResultadosCom"
            android:label="@string/title_activity_resultados_com"
            android:theme="@style/Theme.PruebaPractica2.NoActionBar"></activity>
        <activity android:name=".ui.Resultado" />
        <activity android:name=".Reportes" />
        <activity android:name=".listaErrores" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>